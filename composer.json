{
    "name": "ezsystems/platform-ui-bundle",
    "description": "eZ Platform UI Bundle",
    "license": "GPL-2.0-only",
    "type": "ezplatform-bundle",
    "autoload": {
        "psr-4": {
            "EzSystems\\PlatformUIBundle\\": ""
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/konradoboza/ezpublish-kernel/"
        }
    ],
    "require": {
        "ezsystems/platform-ui-assets-bundle": "^4.2.0",
        "ezsystems/repository-forms": "^1.11",
        "ezsystems/ezpublish-kernel": "dev-EZP-29234-custom_vendor_prefix as 6.13.3",
        "ezsystems/ez-support-tools": "^0.2",
        "zetacomponents/system-information": "^1.1",
        "willdurand/js-translation-bundle": "^2.5",
        "jms/translation-bundle": "^1.3"
    },
    "require-dev": {
        "symfony/symfony": "~2.8|~3.2",
        "phpunit/phpunit": "~4.7",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7",
        "mockery/mockery": "^0.9.5"
    },
    "extra": {
        "_ci_branch-comment_": "Keep ci branch up-to-date with master or branch if on stable. ci is never on github but convention used for ci behat testing!",
        "_ezplatform_branch_for_behat_tests_comment_": "ezplatform branch to use to run Behat tests",
        "_ezplatform_branch_for_behat_tests": "1.13",
        "branch-alias": {
            "dev-master": "1.13.x-dev",
            "dev-tmp_ci_branch": "1.13.x-dev"
        }
    }
}
